Plot: neutron1DOver
	Type: USR-1D
	commands:
		|set title font "Verdana, 35"
		|set key font "Verdana, 30"
		|
		|#Canvas/plot screen size (make everything fit):
		|set lmargin at screen 0.1
		|set rmargin at screen 0.9
		|set bmargin at screen 0.1
		|set tmargin at screen 0.9
		|
		|#Label settings:
		|set ylabel "Neutron fluence [n/cm^2]" font "Verdana, 30"
		|set ylabel offset -10,0
		|set xlabel "Distance from center of DTC [cm]" font "Verdana, 30"
		|set xlabel offset 0,-2
		|
		|#tics/axis numbers:
		|set xtics font "Verdana, 28"
		|set ytics font "Verdana, 28"
		|set format y "10^{%L}"
		|
		|set xrange[0:450]
		|set xtics 0,25,450
		|set xtics offset 0,-0.5
		|
		|set yrange[1E-8:1E-2]
		|#set ytics min,spaceBetween,max
		|set ytics offset -0.5,0
	file.0: DTC_Electronics_WPHAN_32_plot.dat
	file.1: ../pTherapy/DTC_Electronics_WPHAN_32_plot.dat
	grid: 0
	lc.0: red
	lc.1: blue
	lw.0: 3
	lw.1: 3
	name.0: Proton CT
	name.1: Proton Therapy
	ndetectors: 2
	title: 1 MeV neutron equivalent fluence
	y.0: 0
	y.1: 0
	ylog: 1
End
